{
  "name": "bode-rating",
  "version": "1.0.0",
  "description": "A Nest.js lib for rating limit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode production --config webpack.config.js",
    "gen:types": "tsc -p tsconfig.build.json --emitDeclarationOnly --sourceMap",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --debug",
    "test:cov:report": "vitest run --coverage --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damiaoterto/bode-rating.git"
  },
  "keywords": [
    "rating",
    "limit"
  ],
  "author": "Francisco Dami√£o Terto<contato@damiaoterto.dev>",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/damiaoterto/bode-rating/issues"
  },
  "homepage": "https://github.com/damiaoterto/bode-rating#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "express": "^5.1.0",
    "mussurana-cache": "^1.3.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
